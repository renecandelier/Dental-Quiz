          <string>Provide paper and pencil .</string>
          <key>b</key>
          <string>Shout if needed to be heard.</string>
          <key>c</key>
          <string>Face the patient when talking.</string>
          <key>d</key>
          <string>Remove face mask when talking.</string>
          <key>e</key>
          <string></string>
          <key>f</key>
          <string></string>
          <key>g</key>
          <string></string>
          <key>answer</key>
          <string>B.</string>
          <key>rationale</key>
          <string>You should speak to a hearing impaired patient in a normal tone of voice.</string>
    </dict>
    <dict>
          <key>Chapter Number</key>
          <string>10</string>
          <key>Chpater Title</key>
          <string>Ch 10. Special Needs Patients</string>
          <key>Question Number</key>
          <string>17</string>
          <key>Question Title</key>
          <string>All of the following are symptoms of Parkinson’s disease EXCEPT one. Which is the exception?</string>
          <key>a</key>
          <string>Temor.</string>
          <key>b</key>
          <string>Rigidity.</string>
          <key>c</key>
          <string>Bradykinesia.</string>
          <key>d</key>
          <string>Excessive blinking.</string>
          <key>e</key>
          <string></string>
