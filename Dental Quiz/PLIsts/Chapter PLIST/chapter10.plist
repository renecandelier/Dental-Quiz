    </dict>
    <dict>
          <key>Chapter Number</key>
          <string>10</string>
          <key>Chpater Title</key>
          <string>Ch 10. Special Needs Patients</string>
          <key>Question Number</key>
          <string>18</string>
          <key>Question Title</key>
          <string>Generalized widening of the periodontal ligament space can be seen in:</string>
          <key>a</key>
          <string>Scleroderma.</string>
          <key>b</key>
          <string>Grave’s disease.</string>
          <key>c</key>
          <string>Down syndrome.</string>
          <key>d</key>
          <string>Sickle cell anemia.</string>
          <key>e</key>
          <string></string>
          <key>f</key>
          <string></string>
          <key>g</key>
          <string></string>
