    <dict>
          <key>Chapter Number</key>
          <string>7</string>
          <key>Chapter Title</key>
          <string>Pharmacology</string>
          <key>Question Number</key>
          <string>11</string>
          <key>Question Title</key>
          <string>Prolonged use of which of the following can contribute to gingival hyperplasia? </string>
          <key>a</key>
          <string>Digoxin.</string>
