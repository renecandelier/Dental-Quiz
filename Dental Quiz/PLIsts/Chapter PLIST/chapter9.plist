          <string>#4.</string>
          <key>e</key>
          <string></string>
          <key>f</key>
          <string></string>
          <key>g</key>
          <string></string>
          <key>answer</key>
          <string>D.</string>
          <key>rationale</key>
          <string>Film sizes: Pedo (#0), Adult anterior (#1), Adult posterior periapical and bitewing (#2), Adult larger bitewing (#3), Occlucal (#4).</string>
    </dict>
    <dict>
          <key>Chapter Number</key>
          <string>9</string>
          <key>Chapter Title</key>
          <string>Ch 9. Radiology</string>
          <key>Question Number</key>
          <string>15</string>
          <key>Question Title</key>
          <string>Exposure time and mA control:</string>
