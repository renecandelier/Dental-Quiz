          <string>Quality of the beam.</string>
          <key>b</key>
          <string>Photon wavelength.</string>
          <key>c</key>
          <string>Number of x-rays produced.</string>
          <key>d</key>
          <string>Cross section of the beam.</string>
          <key>e</key>
          <string></string>
          <key>f</key>
          <string></string>
          <key>g</key>
          <string></string>
          <key>answer</key>
